# Load the oh-my-zsh's library
antigen use oh-my-zsh

antigen bundle extract
antigen bundle git
antigen bundle github
antigen bundle gradle
antigen bundle mvn
antigen bundle wd

antigen bundle zsh-users/zsh-syntax-highlighting
antigen bundle zsh-users/zsh-completions
#antigen bundle zsh-users/zsh-autosuggestions

antigen bundle rwinch/zsh-plugins copy
antigen bundle rwinch/zsh-plugins gh-update
antigen bundle rwinch/zsh-plugins git-recent
antigen bundle rwinch/zsh-plugins gw
antigen bundle rwinch/zsh-plugins merge-dependency-upgrades
antigen bundle rwinch/zsh-plugins git-add-remote
antigen bundle rwinch/zsh-plugins git-clone-as-owner
antigen bundle rwinch/zsh-plugins git-clone-convention
antigen bundle rwinch/zsh-plugins git-create-worktrees
antigen bundle rwinch/zsh-plugins git-url

# Load the theme
antigen theme robbyrussell

antigen apply

